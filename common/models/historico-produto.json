{
  "name": "HistoricoProduto",
  "plural": "historicosProdutos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "acao": {
      "type": "string"
    },
    "quantidade": {
      "type": "number",
      "default": 0
    },
    "dataValidade": {
      "type": "date"
    },
    "idProduto": {
      "type": "number"
    },
    "idMembroResponsavel": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "usuario": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "idMembroResponsavel",
      "options": {
        "nestRemoting": true
      }
    },
    "produto": {
      "type": "belongsTo",
      "model": "Produto",
      "foreignKey": "idProduto",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
